    "def Generate_Password(num):\n",
    "    password = ''\n",
    "\n",
    "    for n in range(num):\n",
    "        x = random.randint(0, 94)\n",
    "        password += string.printable[x]\n",
    "    return password\n",
    "\n",
    "\n",
